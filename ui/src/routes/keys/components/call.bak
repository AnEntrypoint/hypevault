    <!-- Input to type a string -->
    <input
        type="text"
        bind:value={nodeName}
        class="w-full border rounded-md p-2 mb-2"
    />

    <!-- Button to call the node -->
    <button
        class="bg-blue-500 text-white px-4 py-2 rounded-md hover:bg-blue-600"
        on:click={callNode}>Call Node</button
    >
    <!-- Display the list of call names -->
    <div class="mt-4">
        <h3 class="text-lg font-semibold mb-2">List of Call Names</h3>
        <ul>
            {#each callNames as callName (callName)}
                <li>{callName}</li>
            {/each}
        </ul>
    </div>
